            <string>/// move hero
//move left

if (keyboard_check(ord('A')))
or (keyboard_check(vk_left)){
sprite_index=spr_Hero_Left;
//phy_position (επειδη ειναι physic world)
phy_position_x-=8;

}
if (keyboard_check_released(ord('A')))
or (keyboard_check_released(vk_left)) {
sprite_index = spr_Hero_Left_Stand
}
//move right
if (keyboard_check(ord('D')))
or (keyboard_check(vk_right)) {
sprite_index=spr_Hero_Right;
phy_position_x+=8;

}
if (keyboard_check_released(ord('D')))
or (keyboard_check_released(vk_right)) {
sprite_index = spr_Hero_Right_Stand
}

//move up
if (keyboard_check(ord('W')))
or (keyboard_check(vk_up)) {
sprite_index=spr_Hero_Up;
phy_position_y-=8;

}
if (keyboard_check_released(ord('W')))
or (keyboard_check_released(vk_up)) {
sprite_index = spr_Hero_Up_Stand
}
//move down
if (keyboard_check(ord('S')))
or (keyboard_check(vk_down)) {
sprite_index=spr_Hero_Down;
phy_position_y+=8;

}
if (keyboard_check_released(ord('S')))
or (keyboard_check_released(vk_down)) {
sprite_index = spr_Hero_Down_Stand
}


phy_fixed_rotation = true;

